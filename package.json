{
  "name": "@bedrock-oss/bedrock-boost",
  "version": "0.0.1",
  "description": "A utility package with helper functions for developing add-ons with Script API in Minecraft Bedrock Edition",
  "keywords": [
    "minecraft",
    "bedrock",
    "addon",
    "script-api",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Bedrock-OSS/bedrock-boost.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./playerPolyfill": {
      "import": "./dist/playerPolyfill.mjs",
      "require": "./dist/playerPolyfill.js"
    },
    "./consolePolyfill": {
      "import": "./dist/consolePolyfill.mjs",
      "require": "./dist/consolePolyfill.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "publish": "npm run build && npm publish"
  },
  "author": "Bedrock-OSS",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0"
  },
  "dependencies": {
    "@minecraft/server": "beta"
  },
  "publishConfig": {
    "access": "public"
  }
}